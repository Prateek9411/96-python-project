# plotting without using annotation using python
import matplotlib.pyplot as plt
x=[4,5,6,7,8]
y=[6,5,4,3,5]
plt.scatter(x,y)
plt.show()

# plotting annotation using python
import matplotlib.pyplot as plt
x=[1,2,3,4,5,6]
y=[6,5,4,6,7,6]
labels=["jan","feb","march","april","may","june"]
plt.scatter(x,y)
for i,j in enumerate(labels):
    plt.annotate(j, (x[i]+0.10, y[i]), fontsize=10)
plt.show()
        
        
